system sh/exec.sh -n dnode1 -s start
sql connect

$num = 200
$i = 0
while $i < $num 
  $port = $i + 8000
  $i = $i + 1
  sql create dnode $hostname port $port
endw 

$i = 0
while $i < $num 
  $port = $i + 8000
  $i = $i + 1
  $name = dnode . $port
  system sh/deploy.sh -n $name -i 1
  system sh/cfg.sh -n $name -c serverPort -v $port
  system sh/exec.sh -n $name -s start
endw 

return 

