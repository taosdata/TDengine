$i = 0
$dbPrefix = tb_2m_db
$tbPrefix = tb_2m_tb
$db = $dbPrefix . $i
$tb = $tbPrefix . $i

print =============== step1
sql drop database -x step1
step1:
sql create database $db
sql use $db
sql create table $tb (ts timestamp, speed int) 

$N = 82 

$x = $N * 2
$y = $N
while $x > $y
  $ms = $x . m 
  $xt = - . $x 
  sql insert into $tb values (now - $ms , $xt ) 
  $x = $x - 1
endw 

sql select * from $tb
print sql select * from $tb -> $rows points 
if $rows != $y then 
  return -1
endi

$x = $N
$y = $N * 2
$expect = $N * 2
while $x < $y
  $ms = $x . m 
  sql insert into $tb values (now + $ms , $x ) 
  $x = $x + 1
endw 
sql select * from $tb
print sql select * from $tb -> $rows points 
if $rows != $expect then 
  return -1
endi

print =============== step 2

$result1 = $N
$result2 = $N * 2

$N1 = $result2 + 1
