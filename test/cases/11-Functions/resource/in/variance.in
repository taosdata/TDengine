select VARIANCE(current) from ts_4893.meters
select VARIANCE(current) from ts_4893.meters interval(1d) order by 1 limit 10
select VARIANCE(id) from ts_4893.meters
select VARIANCE(id) from ts_4893.meters interval(1d) limit 10
select VARIANCE(id) from ts_4893.meters where id > 100
select VARIANCE(id) from ts_4893.meters interval(1d) order by 1 limit 10
select variance(null) from ts_4893.meters
select variance(id) from ts_4893.d0
select variance(current) from ts_4893.d0
select variance(voltage) from ts_4893.meters
select variance(voltage) from ts_4893.meters where voltage is not null
select variance(phase) from ts_4893.meters
select variance(phase) from ts_4893.meters where ts between '2023-01-01 00:00:00' and '2023-12-31 23:59:59'
select variance(total_voltage) from (select sum(voltage) as total_voltage from ts_4893.meters group by location)
select round(variance(current), 2) from ts_4893.meters
select pow(variance(current), 2) from ts_4893.meters
select log(variance(voltage) + 1) from ts_4893.meters
select groupid, variance(voltage) from ts_4893.meters group by groupid order by groupid
select location, variance(current) from ts_4893.meters group by location order by location
select location, variance(voltage) from ts_4893.meters group by location order by location
