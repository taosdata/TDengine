  $i = $tblStart
  while $i < $tblEnd
    $tb = tb . $i
    sql drop table if exists $tb
    $i = $i + 1
    $totalRows = $totalRows - $rowsPerTbl
  endw  
  sleep 20000
  goto init_lable 
endi

