sql insert into db.ctb5 values(now, 5, "2")

sql create table db.ctb6 using db.stb tags(6, "102")
sql insert into db.ctb6 values(now, 6, "2")

sql select * from db.stb where t1 = 1
if $rows != 1 then
  return -1
endi

